public class MyClass {
    public static void main(String[] args) {
        int m=0,count=0;
        for (int i=0;i<=2000;i++){
            if (i%3 == 0 ||i%5 == 0 ||i%7 == 0 ||i%53 == 0 ){
             count++;
             m=m+i;
             }
        }
        System.out.println(+m /count);

    }
}
